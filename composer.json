{
    "name": "vdhicts/rebrandly-api-client",
    "description": "API client for the API of Rebrandly",
    "license": "MIT",
    "authors": [
        {
            "name": "Dick van der Heiden",
            "email": "info@vdhicts.nl"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.0",
        "nesbot/carbon": "^3.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^10.0|^11.0",
        "symplify/easy-coding-standard": "^12.3"
    },
    "autoload": {
        "psr-4": {
            "Vdhicts\\Rebrandly\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vdhicts\\Rebrandly\\Test\\": "tests"
        }
    },
    "scripts": {
        "code-style:check": "vendor/bin/ecs",
        "code-style:fix": "vendor/bin/ecs --fix",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/phpunit",
        "test:no-coverage": "vendor/bin/phpunit --no-coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    }
}
